{
  "name": "next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "deploy": "rimraf .open-next .next && opennextjs-cloudflare build && opennextjs-cloudflare deploy",
    "upload": "rimraf .open-next .next && opennextjs-cloudflare build && opennextjs-cloudflare upload",
    "preview": "rimraf .open-next .next && opennextjs-cloudflare build && opennextjs-cloudflare preview",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv ./cloudflare-env.d.ts"
  },
  "browser": {
