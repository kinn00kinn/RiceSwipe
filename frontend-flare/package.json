    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.4.6",
    "prisma": "^6.19.0",
    "rimraf": "^6.1.0",
    "tailwindcss": "^4",
    "typescript": "^5",
    "typescript-eslint": "^8.46.4",
    "wrangler": "^4.47.0"
  }
